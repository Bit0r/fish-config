{
	"log": {
		"access": "/var/log/xray/access.log",
		"error": "/var/log/xray/error.log",
		"loglevel": "warning",
		"dnsLog": true
	},
	"api": {
		"tag": "api",
		"services": [
			//"HandlerService",
			"LoggerService",
			"StatsService"
		]
	},
	"inbounds": [
		{
			"listen": "127.0.0.1",
			"port": 8880,
			"protocol": "dokodemo-door",
			"settings": {
				"address": "127.0.0.1"
			},
			"tag": "api"
		},
		{
			"listen": "0.0.0.0",
			"port": 1080,
			"protocol": "socks",
			"settings": {
				"udp": true
			}
		},
		{
			"listen": "0.0.0.0",
			"port": 8800,
			"protocol": "http"
		},
		{
			"tag": "tproxy",
			"listen": "0.0.0.0",
			"port": 12345,
			"protocol": "dokodemo-door",
			"settings": {
				// "address": "", //
				// "port": 0, //
				"network": "tcp,udp",
				"followRedirect": true
			},
			"sniffing": {
				"destOverride": [
					"http",
					"tls",
					"quic"
				],
				"enabled": true
			},
			"streamSettings": {
				"sockopt": {
					"tproxy": "tproxy"
				}
			}
		}
	],
	"outbounds": [
		{
			"tag": "direct",
			"protocol": "freedom",
			// "sendThrough": "0.0.0.0", //
			// "settings": {
			// 	"domainStrategy": "UseIP", //
			// 	"redirect": ":0" //
			// },
			"streamSettings": {
				"sockopt": {
					"mark": 255
				}
			}
		},
		{
			"tag": "mitmproxy",
			"protocol": "http",
			"settings": {
				"servers": [
					{
						"address": "127.0.0.1",
						"port": 8080
					}
				]
			}
		},
		{
			"tag": "NAT-traversal",
			"protocol": "socks",
			"settings": {
				"servers": [
					{
						"address": "127.0.0.1",
						"port": 1081
					}
				]
			},
			"streamSettings": {
				"sockopt": {
					"mark": 255
				}
			}
		},
		{
			"tag": "dns-out",
			"protocol": "dns",
			"streamSettings": {
				"sockopt": {
					"mark": 255
				}
			}
		}
	],
	"routing": {
		// "domainStrategy": "IPIfNonMatch",
		"domainStrategy": "AsIs",
		"rules": [
			{
				"outboundTag": "mitmproxy",
				"type": "field",
				"domain": [
					"domain:mitm.it"
				]
			},
			{
				"inboundTag": [
					"api"
				],
				"outboundTag": "api",
				"type": "field"
			},
			// {
			// 	"outboundTag": "NAT-traversal",
			// 	"type": "field",
			// 	"domain": [
			// 		"full:zw403-1080ti", // hosts文件中的域名
			// 		"full:zw403-3060",
			// 		"domain:cnki.net"
			// 	]
			// },
			// {
			// 	"outboundTag": "NAT-traversal",
			// 	"type": "field",
			// 	"ip": [
			// 		"172.16.72.0/21"
			// 	]
			// },
			{
				"outboundTag": "dns-out",
				"type": "field",
				"inboundTag": [
					"tproxy"
				],
				"port": 53
			},
			{
				"outboundTag": "direct",
				"type": "field",
				"ip": [
					"geoip:cn",
					"geoip:private"
				]
			},
			{
				"outboundTag": "proxy",
				"type": "field",
				"ip": [
					"geoip:!cn"
				]
			},
			{
				"outboundTag": "proxy",
				"type": "field",
				"domain": [
					"geosite:tiktok"
				]
			},
			{
				"outboundTag": "direct",
				"type": "field",
				"domain": [
					"geosite:cn",
					"geosite:private",
					"domain:stripe.com",
					"domain:cloudflare.com",
					"domain:gitee.io",
					"domain:clarivate.com",
					"domain:clarivate.net",
					"domain:udemy.com",
					"domain:udemycdn.com",
					"domain:litix.io",
					"domain:cookielaw.org",
					"domain:chatkore.com",
					"domain:tmall.hk",
					"domain:ieee.org",
					"domain:gpushare.com",
					"domain:ieee-dataport.org",
					"domain:enmap.org",
					"domain:dlr.de",
					"domain:sectigo.com",
					"domain:spicevids.com"
				]
			},
			{
				"outboundTag": "proxy",
				"type": "field",
				"domain": [
					"geosite:geolocation-!cn",
					"geosite:tld-!cn",
					"domain:org",
					"domain:edu",
					"domain:heymusic.ai",
					"domain:blitz.works",
					"domain:hyper.ai",
					"domain:marc.info",
					"domain:vivaldi.com",
					"domain:fastfiles.cloud",
					"domain:g2plot-vue.opd.cool",
					"domain:onrender.com",
					"domain:activeloop.ai",
					"domain:noname.pub",
					"domain:launchpadcontent.net",
					"domain:onlyoffice.com",
					"domain:zophar.net",
					"domain:einops.rocks",
					"domain:libhunt.com",
					"domain:heynote.com",
					"domain:isnico.com",
					"domain:privacytoolslist.com",
					"domain:direnv.net",
					"domain:apkflash.com",
					"domain:kiwiirc.com",
					"domain:kovidgoyal.net",
					"domain:kdl.dev",
					"domain:rye-up.com",
					"domain:lesbea.com",
					"domain:apkssl.com"
				]
			}
		]
	}
}
